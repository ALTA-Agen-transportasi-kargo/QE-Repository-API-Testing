{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "status": {
      "type": "string"
    },
    "code": {
      "type": "integer"
    },
    "error": {
      "type": "null"
    },
    "links": {
      "type": "object",
      "properties": {
        "self": {
          "type": "string"
        }
      },
      "required": [
        "self"
      ]
    },
    "data": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "CityID": {
              "type": "integer"
            },
            "CityName": {
              "type": "string"
            },
            "ProvID": {
              "type": "integer"
            }
          },
          "required": [
            "CityID",
            "CityName",
            "ProvID"
          ]
        }
      ]
    }
  },
  "required": [
    "status",
    "code",
    "error",
    "links",
    "data"
  ]
}