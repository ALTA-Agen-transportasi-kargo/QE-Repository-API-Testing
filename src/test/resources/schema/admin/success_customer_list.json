{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "status": {
      "type": "string"
    },
    "code": {
      "type": "integer"
    },
    "error": {
      "type": "null"
    },
    "links": {
      "type": "object",
      "properties": {
        "first": {
          "type": "string"
        },
        "last": {
          "type": "string"
        },
        "self": {
          "type": "string"
        }
      },
      "required": [
        "first",
        "last",
        "self"
      ]
    },
    "data": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "id": {
              "type": "integer"
            },
            "email": {
              "type": "string"
            },
            "name": {
              "type": "string"
            },
            "dob": {
              "type": "string"
            },
            "gender": {
              "type": "string"
            },
            "address": {
              "type": "string"
            },
            "phone_number": {
              "type": "string"
            },
            "avatar": {
              "type": "string"
            },
            "role": {
              "type": "string"
            },
            "created_at": {
              "type": "string"
            },
            "updated_at": {
              "type": "string"
            }
          },
          "required": [
            "id",
            "email",
            "name",
            "dob",
            "gender",
            "address",
            "phone_number",
            "avatar",
            "role",
            "created_at",
            "updated_at"
          ]
        }
      ]
    },
    "pagination": {
      "type": "object",
      "properties": {
        "page": {
          "type": "integer"
        },
        "limit": {
          "type": "integer"
        },
        "total_pages": {
          "type": "integer"
        },
        "total_records": {
          "type": "integer"
        }
      },
      "required": [
        "page",
        "limit",
        "total_pages",
        "total_records"
      ]
    }
  },
  "required": [
    "status",
    "code",
    "error",
    "links",
    "data",
    "pagination"
  ]
}